{
  "expo": {
    "name": "CharismaTracker",
    "slug": "charisma-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "charismatracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "frznfrg",
    "extra": {
      "eas": {
        "projectId": "14d8b94a-bf0b-414f-9f77-a990262eda0b"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/14d8b94a-bf0b-414f-9f77-a990262eda0b"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiagentmaker.charismatracker",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to transcribe voice messages.",
        "NSSpeechRecognitionUsageDescription": "This app needs access to speech recognition to convert your voice to text."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/splash-icon-dark.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/splash-icon-light.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aiagentmaker.charismatracker",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
